<div class="panel-body">
    <form class="form-horizontal" role="form" id="dailogForm" action="../roomRecord/roomRecordAdd.do" method="POST">
        <input type="hidden" id="btn_sub" class="btn_sub"/>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">预定日期</label>
            <div class="col-sm-8">
                <input type="text" value="$!dateTool.format("yyyy-MM-dd",$!{roomRecord.date})" name="date" id="date"
                       class="form-control" placeholder="预定日期"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">预定时间段，如0代表00:00-00:30，1代表00:30-1:00</label>
            <div class="col-sm-8">
                <input type="text" value="$!{roomRecord.timePeriod}" name="timePeriod" id="timePeriod"
                       class="form-control" placeholder="预定时间段，如0代表00:00-00:30，1代表00:30-1:00"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">编码:layer-pid</label>
            <div class="col-sm-8">
                <input type="text" value="$!{roomRecord.encode}" name="encode" id="encode" class="form-control"
                       placeholder="编码:layer-pid"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">人员预定表中关联id</label>
            <div class="col-sm-8">
                <input type="text" value="$!{roomRecord.staffRecordId}" name="staffRecordId" id="staffRecordId"
                       class="form-control" placeholder="人员预定表中关联id"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">是否已取消：0-取消，1-未取消</label>
            <div class="col-sm-8">
                <input type="text" value="$!{roomRecord.validStatus}" name="validStatus" id="validStatus" class="form-control"
                       placeholder="是否已取消：0-取消，1-未取消"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">创建时间</label>
            <div class="col-sm-8">
                <input type="text" value="$!dateTool.format("yyyy-MM-dd",$!{roomRecord.createTime})" name="createTime"
                       id="createTime" class="form-control" placeholder="创建时间"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">更新时间</label>
            <div class="col-sm-8">
                <input type="text" value="$!dateTool.format("yyyy-MM-dd",$!{roomRecord.updateTime})" name="updateTime"
                       id="updateTime" class="form-control" placeholder="更新时间"/>
            </div>
        </div>
    </form>
</div>
<script type="text/javascript" src="/resources/js/Validform_v5.3.2.js"></script>
<script type="text/javascript" src="/resources/js/forminit.js"></script> 
