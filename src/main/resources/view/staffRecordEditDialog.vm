<div class="panel-body">
    <form class="form-horizontal" role="form" id="dailogForm" action="../staffRecord/staffRecordEdit.do" method="POST">
        <input type="hidden" id="btn_sub" class="btn_sub"/>
        <input type="hidden" value="$!{staffRecord.id}" name="id" id="id"/>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">预定日期</label>
            <div class="col-sm-8">
                <input type="text" value="$!dateTool.format("yyyy-MM-dd",$!{staffRecord.date})" name="date" id="date"
                       class="form-control" placeholder="预定日期"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">预定开始时间</label>
            <div class="col-sm-8">
                <input type="text" value="$!{staffRecord.startTime}" name="startTime" id="startTime"
                       class="form-control" placeholder="预定开始时间"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">预定结束时间</label>
            <div class="col-sm-8">
                <input type="text" value="$!{staffRecord.endTime}" name="endTime" id="endTime" class="form-control"
                       placeholder="预定结束时间"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">编码:layer-pid</label>
            <div class="col-sm-8">
                <input type="text" value="$!{staffRecord.encode}" name="encode" id="encode" class="form-control"
                       placeholder="编码:layer-pid"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">是否已取消：0-取消，1-未取消</label>
            <div class="col-sm-8">
                <input type="text" value="$!{staffRecord.validStatus}" name="validStatus" id="validStatus"
                       class="form-control"
                       placeholder="是否已取消：0-取消，1-未取消"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">预订人姓名</label>
            <div class="col-sm-8">
                <input type="text" value="$!{staffRecord.staffName}" name="staffName" id="staffName"
                       class="form-control" placeholder="预订人姓名"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">预定用途</label>
            <div class="col-sm-8">
                <input type="text" value="$!{staffRecord.comment}" name="comment" id="comment" class="form-control"
                       placeholder="预定用途"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">创建时间</label>
            <div class="col-sm-8">
                <input type="text" value="$!dateTool.format("yyyy-MM-dd",$!{staffRecord.createTime})" name="createTime"
                       id="createTime" class="form-control" placeholder="创建时间"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">更新时间</label>
            <div class="col-sm-8">
                <input type="text" value="$!dateTool.format("yyyy-MM-dd",$!{staffRecord.updateTime})" name="updateTime"
                       id="updateTime" class="form-control" placeholder="更新时间"/>
            </div>
        </div>
    </form>
</div>
<script type="text/javascript" src="/resources/js/Validform_v5.3.2.js"></script>
<script type="text/javascript" src="/resources/js/forminit.js"></script> 